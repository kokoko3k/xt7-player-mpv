End

Function org_mpris_MediaPlayer2_Player_LoopStatus_Read() As String
  If FMain.TogglePls.value Then 
    Return "Playlist"
      Else
    Return "None"
  Endif
End

Function org_mpris_MediaPlayer2_Player_LoopStatus_Write(p As String)
  FMain.TogglePls.value = (p = "Playlist")
End

