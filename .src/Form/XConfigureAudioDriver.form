          }
          { AlsaDeviceTextBox2 ComboBox
            MoveScaled(18,1,26,4)
            #Translate = False
            Font = Font["Monospace"]
            Expand = True
            List = ["Master", "PCM", "Headphone", "Front", "Center", "Surround", "LFE"]
            Text = "Master"
          }
        }
        { AlsaParseMeHbox2 HBox
          MoveScaled(2,8,46,6)
          AutoResize = True
          Padding = 6
          { AlsaDeviceChk1 CheckBox
            MoveScaled(1,1,15,4)
            #Translate = False
            Tag = "mixer-index="
            Text = "mixer-index"
          }
          { SpinBox1Alsa SpinBox
            MoveScaled(34,1,9,4)
            MaxValue = 10
          }
        }
        Index = 1
        Text = ("Jack")
        { JackParseMeHBox8 HBox
          MoveScaled(1,1,45,6)
          Padding = 8
          { CheckBox18jack CheckBox
            MoveScaled(1,2,17,4)
            #Translate = False
            Tag = "port="
            Text = "Port"
          }
          { TextBox3jack TextBox
            MoveScaled(19,1,22,4)
            #Translate = False
          }
        }
        { JackParseMeHBox1 HBox
          MoveScaled(1,7,45,6)
          Padding = 8
          { CheckBox1jack CheckBox
            MoveScaled(1,2,17,4)
            #Translate = False
            Tag = "name="
            Text = "Name"
          }
          { TextBox4jack TextBox
            MoveScaled(19,1,22,4)
            #Translate = False
            Text = "mpv"
          }
        }
        Index = 2
        Text = ("Pulse")
        { PulseParseMeHbox1 VBox
          MoveScaled(2,2,45,13)
          Padding = 10
          { PulseDriverChk2pulse CheckBox
            MoveScaled(1,1,29,4)
            #Translate = False
            Text = "host:output sink"
          }
          { TextBox6Alsapulse TextBox
            MoveScaled(1,6,41,4)
          }
        }
        { PulseParseMeHbox3 HBox
          MoveScaled(1,17,46,6)
          AutoResize = True
          Padding = 6
          { AlsaDeviceChk3pulse CheckBox
            MoveScaled(1,1,18,4)
            #Translate = False
            Tag = "buffer="
            Text = "buffer"
          }
          { ComboBox1pulse ComboBox
            MoveScaled(21,1,24,4)
            #Translate = False
            List = ["native", "100", "250", "500", "750", "1000", "1250", "1500", "1750", "2000"]
            Text = "250"
          }
        }
        { PulseParseMeHbox2 HBox
          MoveScaled(1,24,46,6)
          AutoResize = True
