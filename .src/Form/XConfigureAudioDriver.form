        { PcmParseMeHBox4 HBox
          MoveScaled(1,8,45,6)
          Tag = "noestimate"
          Padding = 8
          { CheckBox4pcm CheckBox
            MoveScaled(1,1,18,4)
            Tag = "append"
            Text = ("append")
          }
        }
        { PcmParseMeHBox2 HBox
          MoveScaled(1,15,45,6)
          Padding = 8
          { CheckBox2pcm CheckBox
            MoveScaled(1,0,17,4)
            Tag = "file="
            Text = ("File")
          }
          { TextBox8pcm TextBox
            MoveScaled(20,0,19,4)
            Expand = True
          }
          { PcmFileOpen ToolButton
            MoveScaled(40,0,3.2857,3.2857)
